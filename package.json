    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.1",
    "react": "^17.0.2",
    "react-native": "^0.64.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
